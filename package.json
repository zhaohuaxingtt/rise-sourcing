{
  "name": "yuszhou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:win:dev": "vue-cli-service serve --mode dev",
    "start:win:sit": "vue-cli-service serve --mode sit",
    "build:win:dev": "npm install rise@latest && vue-cli-service build --mode dev",
    "build:win:sit": "npm install rise@latest && vue-cli-service build --mode sit",
    "build:win:vmsit": "npm install rise@latest && vue-cli-service build --mode vmsit",
    "build:win:uat": "vue-cli-service build --mode uat",
    "build:win:prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "report": "vue-cli-service build --mode sit --report",
    "macdev": "npm run start:win:dev",
    "build:win:dev:publish": "vue-cli-service build --mode dev && node ./loadersPlugins/publishDevCode.js",
    "conect": "node ./loadersPlugins/publishDevCode.js",
    "fix-memory-limit": "cross-env LIMIT=10240 increase-memory-limit"
  },
  "dependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^4.0.0",
    "element-theme-chalk": "^2.13.2",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "html2canvas": "^1.0.0-rc.7",
    "jsencrypt": "^3.0.0-rc.1",
    "jspdf": "^2.3.1",
    "katex": "^0.13.11",
    "postcss-px2rem": "0.3.0",
    "powerbi-client": "^2.17.2",
    "px2rem-loader": "0.1.9",
    "sass": "^1.26.10",
    "sass-loader": "9.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "2.6.11",
    "vue-count-to": "^1.0.13",
    "vue-katex": "^0.5.0",
    "vue-template-compiler": "2.6.11",
    "webpack-bundle-analyzer": "^3.8.0",
    "rise": "^1.6.0",
    "increase-memory-limit":"^1.0.0",
    "cross-env":"^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
